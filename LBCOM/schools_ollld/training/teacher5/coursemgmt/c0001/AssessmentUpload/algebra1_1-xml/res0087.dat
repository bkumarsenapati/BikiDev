			<choice>
				<choicelabel>4ab</choicelabel>
				<credit></credit>
				<elements>checkbox</elements>
			</choice>
			<choice>
				<choicelabel>4b&lt;sup&gt;2&lt;/sup&gt;</choicelabel>
				<credit></credit>
				<elements>checkbox</elements>
			</choice>
			<choice>
				<choicelabel>4ab&lt;sup&gt;2&lt;/sup&gt;</choicelabel>
				<credit>2</credit>
				<elements>checkbox</elements>
			</choice>
			<choice>
				<choicelabel>none of the above</choicelabel>
