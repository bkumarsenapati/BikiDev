	<item>
		<type>Multiple Choice</type>
		<label>Simplify: 

(x-9) + (x&lt;sup&gt;2&lt;/sup&gt; - x + 17)</label>
			<choice>
				<choicelabel>x - 2</choicelabel>
				<credit></credit>
				<elements>checkbox</elements>
			</choice>
			<choice>
				<choicelabel>x&lt;sup&gt;2&lt;/sup&gt; + 8 </choicelabel>
				<credit>1</credit>
				<elements>checkbox</elements>
			</choice>
			<choice>
				<choicelabel>x + 9</choicelabel>
				<credit></credit>
				<elements>checkbox</elements>
			</choice>
			<choice>
				<choicelabel>x&lt;sup&gt;2&lt;/sup&gt; - 17</choicelabel>
				<credit></credit>
				<elements>checkbox</elements>
			</choice>
	</item>
