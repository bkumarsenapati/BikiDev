	<item>
		<type>Multiple Choice</type>
		<group>5</group>
			<label>Choose the equation that best represents the following sentences.&lt;br&gt;Bob paid $5 less than Tim at a local pizza parlor. Together they spent $12.95.</label>
			<choice>
				<choicelabel>B - 5 = 12.95</choicelabel>
				<credit></credit>
				<elements>radio</elements>
			</choice>
			<choice>
				<choicelabel>B + (B - 5)= 12.95</choicelabel>
				<credit></credit>
				<elements>radio</elements>
			</choice>
			<choice>
				<choicelabel>T + B = 12.95</choicelabel>
				<credit></credit>
				<elements>radio</elements>
			</choice>
			<choice>
				<choicelabel>T + (T - 5) = 12.95</choicelabel>
				<credit>2</credit>
				<elements>radio</elements>
			</choice>
	</item>
	<item>
		<type>Multiple Choice</type>
		<group>5</group>
			<label>Choose the equation that best represents the following sentence.&lt;br&gt;Eighteen more than three times a number is equal to nine more than twice a number.</label>
			<choice>
				<choicelabel>18 + 3 + n = 9 + 2 + n</choicelabel>
				<credit></credit>
				<elements>radio</elements>
			</choice>
			<choice>
				<choicelabel>18 + 3n = 9 + 2n</choicelabel>
				<credit>2</credit>
				<elements>radio</elements>
			</choice>
			<choice>
				<choicelabel>18(3n) = 9(2n)</choicelabel>
				<credit></credit>
				<elements>radio</elements>
			</choice>
			<choice>
				<choicelabel>18 + 3n = 9 + (n + 2)</choicelabel>
				<credit></credit>
				<elements>radio</elements>
			</choice>
	</item>
